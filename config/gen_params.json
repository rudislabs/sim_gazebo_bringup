{	"setup":{
		"autopilot": {},
		"gazebo": {
			"gazebo_models": {
				"gazebo_model_repo_0":{
					"repo": "git@github.com:rudislabs/nxp_gazebo.git",
					"version": "nxp-summer",
					"name": "nxp_gazebo"
				},
				"gazebo_model_repo_1":{
					"repo": "git@github.com:osrf/gazebo_models.git",
					"version": "master",
					"name": "osrf/models"
				},
				"gazebo_model_repo_2":{
					"repo": "git@github.com:NXPHoverGames/gazebo_apriltag.git",
					"version": "master",
					"name": "gazebo_apriltag"
				}
			},
			"gazebo_plugins": {}
		},
		"ros2": {
			"ros2_repo_0":{
				"repo": "git@github.com:rudislabs/nxp_cup_interfaces.git",
				"version": "aim",
				"build_package": "nxp_cup_interfaces",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_1":{
				"repo": "git@github.com:rudislabs/nxp_cup_vision.git",
				"version": "nxp-summer",
				"build_package": "nxp_cup_vision",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_2":{
				"repo": "git@github.com:rudislabs/aim_line_follow_c.git",
				"version": "main",
				"build_package": "aim_line_follow_c",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_3":{
				"repo": "git@github.com:christianrauch/apriltag.git",
				"version": "master",
				"build_package": "apriltag",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_4":{
				"repo": "git@github.com:christianrauch/apriltag_msgs.git",
				"version": "master",
				"build_package": "apriltag_msgs",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_5":{
				"repo": "git@github.com:NXPHoverGames/apriltag_ros.git",
				"version": "master",
				"build_package": "apriltag_ros",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			}
		},
		"system": {
			"set_environment": {
				"environment_0": {
					"variable": "GAZEBO_MODEL_DATABASE_URI",
					"value": "",
					"method": "overwrite"
				},
				"environment_1": {
					"variable": "GAZEBO_RESOURCE_PATH",
					"value": "/usr/share/gazebo-11",
					"method": "prepend"
				},
				"environment_2": {
					"variable": "GAZEBO_PLUGIN_PATH",
					"value": "/opt/ros/foxy/lib",
					"method": "prepend"
				}
			}
		}
	},
	"verbose": {
		"gzclient": true,
		"gzserver": true
	},
	"nodes": {
		"ros2_node_0":{
			"timing": "post-spawn",
			"package": "nxp_cup_vision",
			"executable": "nxp_track_vision",
			"name": "nxp_track_vision_0",
			"output": "screen",
			"parameters": [	{"pyramid_down": 0},
							{"camera_image": "pixy2_cmucam5_0"},
							{"debug_image": "debugImage0"},
							{"namespace": "cupcar0"},
							{"mask_ratio_array": [1.0, 0.4]}

						]
		},
		"ros2_node_1":{
			"timing": "post-spawn",
			"package": "rqt_image_view",
			"executable": "rqt_image_view",
			"name": "rqt_image_view_0",
			"output": "screen",
			"parameters": []
		},
		"ros2_node_2":{
			"timing": "post-spawn",
			"package": "aim_line_follow_c",
			"executable": "aim_line_follow_c",
			"name": "line_follower_0",
			"output": "screen",
			"parameters": [	{"start_delay": 5.0},
							{"camera_vector_topic": "/cupcar0/PixyVector"},
							{"linear_velocity": 1.5},
							{"angular_velocity": -0.6},
							{"single_line_steer_scale": 0.6}
						]
		}
	},
	"world_params": {
		"gazebo_name": "nxp_gazebo",
