			"sun_utc_date": "now",
			"ambient_light": 0.5,
			"background_light": 0.15,
			"shadows": 0,
			"cloud_speed": 12,
			"wind_speed": "NotSet",
			"embedded_models": {
				"embed_model_0": {
					"model": "nxp_raceway_octagon",
