				},
				"embed_model_1": {
					"model": "Apriltag36_11_00000",
					"name": "Apriltag36_11_00000_0",
					"pose": "0.33 0 0 0 0 0"
				}
			},
			"video_widget": "NotSet",
			"set_physics": 0,
			"update_rate": 100,
			"realtime_factor": 1.0,
			"ode_threads": 4
		}
	},
	"models": {
		"model_params_0": {
			"gazebo_name": "nxp_gazebo",
			"spawn_pose": [0, 0, 0.1, 0, 0, 0],
			"instance": 0,
			"generate_params":{
				"controller": "ros2",
				"base_model": "nxp_cupcar",
				"sdf_version": 1.7,
				"namespace": "cupcar0",
				"model_name": "NotSet",
				"sensors": {
					"camera_0":{
						"method": "include",
						"pose": "-.09 0 .35 0 1.0 0",
						"params":{
							"base_model": "pixy2_cmucam5",
							"model_name": "pixy2_cmucam5_0",
							"near": 0.05,
							"far": 300,
							"width": 156,
							"height": 102,
							"update_rate": 30
						},
						"joint": {
							"type":"fixed",
							"axis":""
						}
					}
				},
				"hq_wheel": "0",
				"hq_frame": "0"
			}
		}
	}
}
